<script>
$(document).ready(function () {
    // Always show the splash screen on page load
    $('#loader-container').css('display', 'flex'); // Show splash screen
    setTimeout(function() {
        $('.splash-wrapper img').fadeIn(300); // Fade in the image
    }, 300);

    // Trigger swipe-out animation after 1.5 seconds
    setTimeout(function() {
        $('#loader-container').css({
            animation: 'swipeOut 0.5s forwards'
        });

        // Remove the element from the DOM after animation ends
        setTimeout(function() {
            $('#loader-container').remove();
        }, 500); // Match this duration to the animation time
    }, 1500);
});
